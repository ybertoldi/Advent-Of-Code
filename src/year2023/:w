from pprint import pprint

maps = []
buf = [ln.strip() for ln in open("inputs/day5.txt", "r").readlines()]
seeds = [int(s) for s in buf[0].split()[1:]]

i = 2
curmap = []
while i < len(buf):
    if not buf[i]:
        maps.append(curmap)
        curmap = []
    else:
        curmap.append([int(v) for v in buf[i].split()])
    i += 1

if curmap:
    maps.append(curmap)


pprint(buf)
